{
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6"
    ],
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "paths": { "*": ["node_modules/*"]}
  },
  "include": ["src/**/*"]
}